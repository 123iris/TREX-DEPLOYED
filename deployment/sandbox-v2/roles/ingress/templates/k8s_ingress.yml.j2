apiVersion: networking.k8s.io/v1beta1
kind: Ingress
metadata:
  name: main-ingress
  namespace: default

spec:
  rules:
  - http:
      paths:
      - path: /config
        backend:
          serviceName: kernel-config-server 
          servicePort: 80
      - path: /v1/authmanager
        backend:
          serviceName: {{kernel_auth_name}}
          servicePort: 80
      - path: /preregistration/v1
        backend:
          serviceName: {{pre_reg_login_name}}
          servicePort: 80

